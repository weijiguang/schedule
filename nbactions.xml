<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>CUSTOM-jetty:run debug mode</actionName>
            <displayName>jetty:run debug mode</displayName>
            <goals>
                <goal>jetty:run</goal>
            </goals>
            <properties>
                <jpda.listen>maven</jpda.listen>
                <netbeans.deploy.debugmode>true</netbeans.deploy.debugmode>
            </properties>
        </action>
        <action>
            <actionName>CUSTOM-run jetty</actionName>
            <displayName>run jetty</displayName>
            <goals>
                <goal>clean</goal>
                <goal>jetty:run</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-stop jetty</actionName>
            <displayName>stop jetty</displayName>
            <goals>
                <goal>jetty:stop</goal>
                
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-rerun jetty</actionName>
            <displayName>rerun jetty</displayName>
            <goals>
                <goal>jetty:stop</goal>
                <goal>clean</goal>
                <goal>jetty:run</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-clean package</actionName>
            <displayName>clean package</displayName>
            <goals>
                <goal>clean:clean</goal>
                <goal>package</goal>
            </goals>
        </action>
        
    </actions>
